  Item:
    type: object
    properties:
      brand_id:
        description: the brand that the item is associated with
        type: integer
        example: 6
      brand_name:
        description: name of the brand
        type: string
        example: StellaSport
      category_id:
        description: the category that the item is associated with
        type: integer
        example: 58
      category_name:
        description: name of the category
        type: string
        example: Tank Top
      description:
        description: the description of the item
        type: string
        example: >-
          soft and strechy material, versatile solid color, long length is
          perfect for layering, scoop neck front and back, imported, 95% cotton
          and 5% spandex, machine wash
      details:
        description: a dictionary of item details - often empty
        type: object
        example: '{}'
      flagged:
        description: whether the item is flagged or not
        type: integer
        example: 0
      item_id:
        description: the FINDMINE item id for the item
        type: integer
        example: 159820
      item_url:
        description: url of the item
        type: string
        format: url
        example: 'http://www.somestore/com/product/wonderful-dress'
      pid:
        description: product id of the item
        type: string
        example: AKD7848F
      price:
        description: price of the item
        type: number
        format: float
        example: 49.87
      sale_id:
        description: sale id of the item
        type: integer
        example: 5
      sale_name:
        description: sale status of the item
        type: string
        example: Clearance
      season_id:
        description: season id of the item
        type: integer
        example: 7
      season_name:
        description: season of the item
        type: string
        example: Fall17
      size_id:
        description: size id of the item
        type: integer
        example: 9
      size_name:
        description: size of the item
        type: string
        example: Plus Size
      sport_id:
        description: sport id of the item
        type: integer
        example: 12
      sport_name:
        description: sport of the item
        type: string
        example: Basketball
      stock:
        description: whether the item is in stock or out of stock
        type: integer
        example: 1
      store_id:
        description: store id of the item
        type: integer
        example: 10
      store_name:
        description: name of store that item is from
        type: string
        example: John Varvatos
      team_id:
        description: team id of the item
        type: integer
        example: 1
      team_name:
        description: team of the item
        type: string
        example: Default Team
      title:
        description: item title
        type: string
        example: Scoop Neck Plus Size Tank Top
      uni:
        description: a unique identifier for the item
        type: string
        example: 25affc885754ga7
      date_created:
        description: the datetime that the item was created
        type: string
        format: datetime
        example: '2017-06-21 12:04:12'
      date_updated:
        description: the datetime that the item was last updated
        type: string
        format: datetime
        example: '2017-06-23 16:27:02'
      update_count:
        description: the number of times the item has been updated
        type: integer
        example: 2
      user_id:
        description: FINDMINE user id of the last person to update the item
        type: integer
        example: 162
      score:
        description: the relevance score of the result
        type: integer
        example: null
      href:
        description: path of the item
        type: string
        example: 'https://www.findmine.com/api/v1/items/159820'
      images:
        $ref: '#/definitions/Images'
      colors:
        $ref: '#/definitions/Colors'
    xml:
      name: Item
  ItemsPost:
    type: object
    properties:
      url:
        description: url of the product you wish to identify or add to the system
        type: object
        example: 'http://www.somestore/com/product/wonderful-dress'
    xml:
      name: ItemsPost
  MatchSetsPostUrl:
    type: object
    properties:
      url:
        description: URL of the item needing identified or to be added to the system
        type: object
        example: 'http://www.somestore/com/product/wonderful-dress'